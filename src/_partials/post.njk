{%extends "_layout.njk" %} 

{% block content %}
<article>
  <header>
    <h1 class="post-title">{{ page.title }}</h1>
    <time datetime="{{page.date.datetime}}" class="date">
      {{ page.date.datetime }}
    </time>
  </header>
  Slug {{ page.slug }}
  <p>{{ content }}</p>
</article>
{% endblock %}
